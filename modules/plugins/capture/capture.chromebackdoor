%title%Capture window v1%title%

%content%
    $.get(server_web+"relais/html2canvas", function(data){
            var canv = document.createElement('script');
            canv.type = 'text/javascript';
            canv.async = true;
            canv.id = "tototo";
            canv.innerHTML = data;
            document.body.appendChild(canv);
    })    
%content%

